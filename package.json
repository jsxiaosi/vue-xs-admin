{
  "name": "vue-xs-admin",
  "version": "1.8.0",
  "packageManager": "pnpm@8.1.0",
  "author": {
    "name": "小斯斯",
    "email": "1531733886@qq.com",
    "url": "https://github.com/SuperCuteXiaoSi"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "staging": "vite build --mode staging",
    "serve": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "visualizer": "cross-env REPORT=true npm run build",
    "log": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:gzip": "http-server dist --cors --gzip -c-1",
    "test:br": "http-server dist --cors --brotli -c-1",
    "lint:eslint": "eslint \"{src,mock,build}/**/*.{vue,ts,js,tsx,jsx}\" package.json --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,css,scss,vue,ts,tsx,jsx,html,md}\"",
    "lint:stylelint": "stylelint --fix scss \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "lint:staged": "lint-staged",
    "cz": "czg",
    "prepare": "husky install",
    "push": "scp -r dist/* root@服务器公网IP:/服务器资源存放地址",
    "build:push": "npm run build && npm run push"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@jsxiaosi/utils": "^0.0.16",
    "@logicflow/core": "^1.2.10",
    "@logicflow/extension": "^1.2.10",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "docx-preview": "^0.1.18",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.9",
    "es6-promise": "^4.2.8",
    "i18next": "^23.4.1",
    "intro.js": "^7.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "marked": "^4.3.0",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-pdf-embed": "^1.1.6",
    "vue-router": "^4.2.4",
    "vue-types": "^5.1.1",
    "vuex": "^4.1.0",
    "wangeditor": "^4.7.15"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@jsxiaosi/commitlint-config": "^0.0.14",
    "@jsxiaosi/eslint-config-prettier": "^0.0.14",
    "@jsxiaosi/eslint-config-vue": "^0.0.14",
    "@types/core-js": "^2.5.5",
    "@types/intro.js": "^5.1.1",
    "@types/lodash-es": "^4.17.8",
    "@types/marked": "^4.3.0",
    "@types/node": "*",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.14",
    "conventional-changelog-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "czg": "^1.7.0",
    "echarts-wordcloud": "^2.1.0",
    "eslint": "^8.46.0",
    "eslint-plugin-jsonc": "^2.9.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "kolorist": "^1.8.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.1.6",
    "unplugin-element-plus": "^0.8.0",
    "vite": "^4.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.7.35",
    "vite-plugin-linter": "^2.0.2",
    "vite-plugin-mock": "3.0.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.34.1",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8",
    "xlsx": "^0.18.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "source-map-*",
        "stable",
        "uuid",
        "urix"
      ]
    }
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "lint-staged": {
    "˜{src,mock,build}/**/*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
