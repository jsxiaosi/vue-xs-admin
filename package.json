  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "staging": "vite build --mode staging",
    "serve": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "visualizer": "cross-env REPORT=true npm run build",
    "log": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:gzip": "http-server dist --cors --gzip -c-1",
    "test:br": "http-server dist --cors --brotli -c-1",
    "lint:eslint": "eslint \"{src,mock,build}/**/*.{vue,ts,js,tsx,jsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,css,scss,vue,ts,tsx,jsx,html,md}\"",
    "lint:stylelint": "stylelint --fix scss \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "lint:staged": "lint-staged",
    "cz": "czg",
    "prepare": "husky install",
    "push": "scp -r dist/* root@服务器公网IP:/服务器资源存放地址",
    "build:push": "npm run build && npm run push"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@logicflow/core": "^1.1.29",
    "@logicflow/extension": "^1.1.29",
    "@vueuse/core": "^9.3.0",
    "axios": "^1.1.3",
    "dayjs": "^1.11.5",
    "echarts": "^5.4.0",
    "element-plus": "^2.2.18",
    "es6-promise": "^4.2.8",
    "i18next": "^21.10.0",
    "image-conversion": "^2.1.1",
    "intro.js": "^6.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "marked": "^4.1.1",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.0.23",
    "prettier": "^2.7.1",
    "qs": "^6.11.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "vue-types": "^4.2.1",
    "vuex": "^4.1.0",
    "wangeditor": "^4.7.15"
  },
  "devDependencies": {
    "@ehutch79/vite-eslint": "^0.0.1",
    "@esbuild-kit/cjs-loader": "^2.4.0",
    "@jsxiaosi/commitlint-config": "^0.0.8",
    "@jsxiaosi/eslint-config-prettier": "^0.0.8",
    "@jsxiaosi/eslint-config-vue": "^0.0.8",
    "@types/core-js": "^2.5.5",
    "@types/crypto-js": "^4.1.1",
    "@types/intro.js": "^5.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/marked": "^4.0.7",
    "@types/node": "18.8.0",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.41",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "czg": "^1.3.11",
    "eslint": "^8.25.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "kolorist": "^1.6.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.0.0",
    "postcss-scss": "^4.0.5",
    "pretty-quick": "^3.1.3",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.4",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-macros": "^0.12.1",
    "vite": "^3.1.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-linter": "^1.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.24.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.8",
    "xlsx": "^0.18.5"
  },
  "license": "MIT",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "source-map-*",
        "stable",
        "uuid",
        "urix"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "˜{src,mock,build}/**/*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "14.x || 16.x"
  }
}
